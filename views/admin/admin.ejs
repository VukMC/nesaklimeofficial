<section class="narudzbineeeeeeeeeee">
    <h2 class="admin-narudzbine">Sve porudžbine</h2>
    <ul>
        <% let number = 1 %>
        <% data.forEach(narudzbina => { %>
            <li class="listni-item rounded-3 p-2 narudzbina-broj-x">
                <a class="narudzbina-link" href="/narudzbina/<%= narudzbina._id %>">
                    <div class="helping-div-1">
                        <span>Porudžbina <%= number++ %></span>
                        <span class="porudzbina-list--date">Datum: <%= narudzbina.createdAt.toDateString() %></span>
                    </div>
                </a>
                <div class="helping-div-2">
                    <button class="uspeh"><svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" fill="currentColor" class="bi bi-check" viewBox="0 0 16 16">
                        <path d="M10.97 4.97a.75.75 0 0 1 1.07 1.05l-3.99 4.99a.75.75 0 0 1-1.08.02L4.324 8.384a.75.75 0 1 1 1.06-1.06l2.094 2.093 3.473-4.425z"/>
                    </svg></button>
                    <button class="neuspeh"><svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" fill="currentColor" class="bi bi-x-lg" viewBox="0 0 16 16">
                        <path d="M2.146 2.854a.5.5 0 1 1 .708-.708L8 7.293l5.146-5.147a.5.5 0 0 1 .708.708L8.707 8l5.147 5.146a.5.5 0 0 1-.708.708L8 8.707l-5.146 5.147a.5.5 0 0 1-.708-.708L7.293 8z"/>
                    </svg></button>
                </div>
            </li>
        <% }); %>
    </ul>
</section>